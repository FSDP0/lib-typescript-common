{
  "name": "@fsdp0/common",
  "version": "0.0.1",
  "types": "dist/lib/types/index.d.ts",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "LeeGueTae",
    "email": "fsdp0dev@gmail.com",
    "url": "https://github.com/FSDP0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FSDP0/lib-typescript-common.git"
  },
  "config": {
    "git": {
      "branch": "dev"
    }
  },
  "publishConfig": {
    "access": "restricted"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/lib/types/index.d.ts"
    }
  },
  "os": [
    "win32"
  ],
  "packageManager": "yarn@1.22.21",
  "keywords": [
    "fsdp0",
    "custom"
  ],
  "scripts": {
    "preversion": "git checkout %npm_package_config_git_branch%",
    "git:staging": "git add .",
    "git:commit": "git commit --allow-empty -m \"[FSDP-COMM-0000] Auto Commit\"",
    "git:tagging": "git tag -f v%npm_package_version%-lw",
    "git:push": "git push origin v%npm_package_version%-lw",
    "git": "yarn git:staging && yarn git:commit && yarn git:tagging && yarn git:push",
    "rmdir": "rimraf dist",
    "compile:esm": "tsc --project tsconfig.esm.json",
    "compile:cjs": "tsc --project tsconfig.cjs.json",
    "compile": "yarn rmdir && yarn compile:esm && yarn compile:cjs",
    "prepublishOnly": "yarn compile",
    "postpublish": "yarn git"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "rimraf": "^6.0.1"
  },
  "dependencies": {
    "@fsdp0/comm": "^0.0.1"
  }
}
